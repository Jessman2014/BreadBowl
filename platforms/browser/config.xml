<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dahirkanehl.breadbowl" version="0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Bread Bowl</name>
    <description>
        A basic expense tracking app using cross platform Phonegap.
    </description>
    <author email="dahir-ka.jess@uwlax.edu" href="http://jessedahirkanehl.azurewebsites.net/">
        Jesse Dahir-Kanehl
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
